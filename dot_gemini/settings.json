{
    "ide": {
        "hasSeenNudge": true,
        "enabled": true
    },
    "mcpServers": {
        "hatago": {
            "command": "npx",
            "args": [
                "-y",
                "@himorishige/hatago-mcp-hub",
                "serve",
                "--stdio",
                "--config",
                "$HOME/.config/hatago/hatago.config.json"
            ]
        }
    },
    "general": {
        "preferredEditor": "vscode"
    },
    "security": {
        "auth": {
            "selectedType": "oauth-personal"
        }
    },
    "ui": {
        "theme": "Dracula"
    }
}