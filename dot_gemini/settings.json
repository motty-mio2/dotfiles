{
  "ide": {
    "hasSeenNudge": true,
    "enabled": true
  },
  "mcpServers": {
    "chrome-devtools": {
      "command": "npx",
      "args": [
        "chrome-devtools-mcp@latest"
      ]
    },
    "mcpc": {
      "command": "npx",
      "args": [
        "-y",
        "github:motty-mio2/mcpc#main",
        "serve"
      ]
    }
  },
  "general": {
    "preferredEditor": "vscode",
    "previewFeatures": true,
    "enablePromptCompletion": true
  },
  "security": {
    "auth": {
      "selectedType": "oauth-personal"
    }
  },
  "ui": {
    "theme": "Dracula"
  },
  "output": {
    "format": "text"
  }
}